{
  "id": "ai-agents",
  "name": "ai-agents",
  "displayName": "Hansen AI Agents Framework",
  "version": "1.0.0",
  "description": "Intelligent AI agents for automation: ContentAgent, ClientAgent, InvoiceAgent, ProjectAgent",
  "category": "automation",
  "status": "active",
  "author": "Cato Hansen",
  "license": "PROPRIETARY",
  "tags": ["ai", "automation", "agents", "machine-learning"],
  "dependencies": ["nora", "client-management", "content-management"],
  "features": [
    "ContentAgent - SEO optimization, alt-text generation",
    "ClientAgent - Auto-responses, follow-ups",
    "InvoiceAgent - Auto-invoicing, payment reminders",
    "ProjectAgent - Status updates, deadline tracking",
    "Agent orchestration",
    "Learning from feedback",
    "Hansen Security integration"
  ],
  "pricing": {
    "model": "subscription",
    "plans": {
      "professional": {
        "name": "Professional",
        "price": 1999,
        "currency": "NOK",
