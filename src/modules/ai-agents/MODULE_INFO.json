      "enterprise": {
        "name": "Enterprise",
        "price": "custom",
        "currency": "NOK",
        "features": ["All agents", "Custom agents", "Dedicated support", "SLA"]
      }
    }
  },
  "api": {
    "endpoints": [
      "POST /api/ai-agents/content/optimize-seo",
      "POST /api/ai-agents/content/generate-alt-text",
      "POST /api/ai-agents/client/auto-respond",
      "POST /api/ai-agents/invoice/generate",
      "POST /api/ai-agents/project/update-status"
    ]
  },
  "sdk": {
    "available": true,
    "npm": "@hansen/ai-agents-sdk",
    "documentation": "/modules/ai-agents/README.md"
  }
